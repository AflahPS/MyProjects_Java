
public class Sample extends Thread {

	int a;
	Sample(int a){
		this.a=a;{
	}
		
		
	}
	
	@Override
	public void run() {
		super.run();
		for (int i = 0; i < 10; i++) {

			System.out.println("Thread " + i+ "Object "+a);
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}

	}
}
